generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model public_usuarios {
  nome  String @db.VarChar(50)
  email String @db.VarChar(50)
  senha String @db.VarChar(50)
  id    String @id(map: "usuarios_pk") @db.VarChar(70)

  @@map("public.usuarios")
}
